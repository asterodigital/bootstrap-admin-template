{
  "$schema": "https://json.schemastore.org/prettierrc",
  "plugins": ["prettier-plugin-astro"],
  "semi": true,
  "singleQuote": false,
  "printWidth": 100,
  "tabWidth": 2,
  "trailingComma": "none",
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ]
}
